  "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "noImplicitAny": false,
      "baseUrl": ".",
      "paths": {
          "*": ["**/*.d.ts"]
      },
      "outDir": "./dist"
  },
  "exclude": [
      "logs",
      "node_modules",
  ]
}
